<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorViewBg"
    android:scrollbars="none"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".person_list.PersonListActivity">

    <TextView
        android:id="@+id/tv_person_url"
        android:layout_width="364dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView12"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="26dp"
        android:layout_marginTop="13dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="364dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_person_location"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="12dp"
        android:fontFamily="sans-serif-black"
        android:text="Url" />

    <TextView
        android:id="@+id/tv_person_location"
        android:layout_width="364dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView10"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="12dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="364dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_person_name"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="26dp"
        android:layout_marginTop="13dp"
        android:fontFamily="sans-serif-black"
        android:text="Localizacion" />

    <TextView
        android:id="@+id/tv_person_name"
        android:layout_width="364dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="23dp"
        android:layout_marginTop="14dp" />

    <TextView
        android:id="@+id/tv_person_user"
        android:layout_width="364dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="22dp"
        android:layout_marginTop="13dp"
        android:text="" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="367dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_person_id"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="22dp"
        android:layout_marginTop="13dp"
        android:text="Usuario" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="367dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_person_id"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="22dp"
        android:layout_marginTop="13dp"
        android:text="Usuario" />

    <TextView
        android:id="@+id/tv_label_id"
        android:layout_width="369dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="21dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="sans-serif-black"
        android:text="ID" />

    <TextView
        android:id="@+id/tv_person_id"
        android:layout_width="369dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_label_id"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="21dp"
        android:layout_marginTop="13dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="367dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_person_id"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="22dp"
        android:layout_marginTop="13dp"
        android:text="Usuario" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="364dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_person_user"
        android:layout_alignParentStart="true"
        android:layout_marginStart="22dp"
        android:layout_marginTop="13dp"
        android:fontFamily="sans-serif-black"
        android:text="Nombre Real" />

</RelativeLayout>